    重置对话框
    */

.modal-Change-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
}

.modal-Change-dialog {
    width: 80%;
    overflow: hidden;
    position: fixed;
    top: 50%;
    left: 10%;
    z-index: 9999;
    background: #f9f9f9;
    margin: -180rpx 0;
    border-radius: 36rpx;
    border: 1px solid #444;
}

.modal-Change-title {
    padding: 30rpx 0;
    font-size: 46rpx;
    font-weight: 800;
    color: #1d89e4;
    text-align: center;
    border-bottom: 1px solid #e5e5e5;
}

.modal-Change-content {
    padding: 50rpx 32rpx;
    font-size: 32rpx;
}

.modal-Change-footer {
    display: flex;
    flex-direction: row;
    height: 86rpx;
    border-top: 1px solid #dedede;
    font-size: 34rpx;
    line-height: 86rpx;
}

.Change-btn-cancel {
    width: 50%;
    color: #666;
    text-align: center;
    border-right: 1px solid #dedede;
}

.Change-btn-confirm {
    width: 50%;
    color: #fff;
    text-align: center;
    background: #74cf9f;
}

.go {
    width: 80rpx;
    height: 50rpx;
    z-index: 9999999999999999999;
    position: absolute;
    right: 40rpx;
    top: 30rpx;
}

button::after {
    border: 0;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.confirmBtn{
    display: inline-block;
    width: 60%;
    height: 70rpx;
    background: #1e88e5;
    color: #fff;
}

button[disabled][type="default"], wx-button[disabled]:not([type]) {
    color:#fff;
    background-color:gray;
}