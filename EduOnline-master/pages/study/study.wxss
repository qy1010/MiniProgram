/* pages/study/study.wxss */

.page {
    background: #f6f6f6;
}

.header {
    width: 100%;
    height: 200rpx;
    background: #fff;
    box-shadow: 0px 3px 3px #b9bdc0;
    position: fixed;
    z-index: 111111111;
}

.header image {
    width: 70%;
    height: 150rpx;
    margin: 0 auto;
    display: block;
    position: relative;
    top: 25rpx;
}

.teachingVideos {
    width: 100%;
    min-height: 500rpx;
    padding-top: 250rpx;
    text-align: center;
}

.teachingVideos video {
    margin: 10rpx auto;
    z-index: 110;
    width: 95%;
    height: 350rpx;
}

.start {
    width: 100%;
    height: 200rpx;
    background: #fff;
    box-shadow: 0px 3px 3px #b9bdc0;
    text-align: center;
}

.start .testing {
    width: 100%;
    height: 100rpx;
}

.start .testing image {
    width: 35%;
    height: 70rpx;
    margin-top: 25rpx;
}

.start .startBtn {
    width: 100%;
    height: 100rpx;
}

.start .startBtn image {
    width: 340rpx;
    height: 60rpx;
    margin: 20rpx 0;
}
.btn{
    width: 100%;
    height: 100rpx;
    text-align: center
}
.title{
    width: 130rpx;
    height: 80rpx;
    line-height: 80rpx;
    float: left;
    display: block;
    margin:0 25rpx 10rpx;
    font-size: 0.8rem;
    padding: 0;
}

.thistrue{
    background: #1e88e5;
    color: #fff;
}
.videoPro{
    width: 100%;
    height: 80rpx;
}
.thiseng{
    background-image: url('https://api.jules.xin/images/eng/start.png')
}
.thischs{
    background-image: url('https://api.jules.xin/images/startTest.png')
}
.startbtn{
    width:340rpx;
    height:60rpx;
    margin:20rpx auto;
    background-size: 100% 100%;
    text-indent:-99999em;
    overflow:hidden;
}