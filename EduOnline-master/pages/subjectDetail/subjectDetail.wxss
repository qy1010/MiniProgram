    text-align: center;
    background: red;
}

.modal-video-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
}

.modal-video {
    width: 90%;
    height: 600rpx;
    margin: 0 auto;
    text-align: center;
    position: fixed;
    top: 6%;
    left: 5%;
    z-index: 9999;
    background: #fff;
    border-radius: 10rpx;
}

.modal-video video {
    width: 100%;
    height: 450rpx;
    margin-top: 50rpx;
}

.modal-video-footer {
    height: 100rpx;
    background: #1d89e4;
    line-height: 100rpx;
    font-size: 0.8rem;
    color: #fff;
}

.video-btn-cancel {
    height: 100rpx;
    background: #1d89e4;
    line-height: 100rpx;
    font-size: 0.8rem;
    color: #fff;
}

/*播放按钮*/

.playBtn {
    width: 150rpx;
    height: 150rpx;
    line-height: 150rpx;
    background: #1d89e4;
    border-radius: 50%;
    color: #fff;
    font-size: 0.8rem;
    padding: 0;
    margin: 0 auto;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

button::after {
    border: 0;
}

.storybook {
    width: 90%;
    height: 780rpx;
    background: #fff;
    margin: 30rpx auto;
    text-align: center;
}

.story_desc {
    width: 100%;
    height: 300rpx;
    font-size: 0.7rem;
}

.story_desc text {
    width: 100%;
    display: inline-block;
    text-align: left;
    text-indent: 25px;
}

.pageTurn {
    width: 100%;
    display: flex;
    height: 100rpx;
}

.pageTurn  .image {
    flex: 1;
    height: 100rpx;
}

.pageTurn .storyPage {
    flex: 1;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 0.7rem;
    font-weight: 600;
}

.pageTurn  .image image {
    width: 200rpx;
    height: 60rpx;
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}

.sequence {
    display: block;
    width: 5%;
    height: auto;
    float: left;
}

.con {
    width: 92%;
    display: block;
    float: left;
    text-indent: 0;
    padding-left: 2.5%;
    margin-bottom: 10rpx;
}

.myAudio{
    margin:0 auto
}

.refuseCont{
    width: 100rpx;
    height: 100rpx;
    position: fixed;
    bottom: 100px;
}
.refuseCont button{
    width: 100rpx;
    height: 100rpx;
    padding: 0;
    margin: 0;
    font-size: 0.7rem;
    line-height:100rpx;
    background:#dedede;
    border-radius: 50%;
    box-shadow: 2px 0 3px 0 #dedddd;
}