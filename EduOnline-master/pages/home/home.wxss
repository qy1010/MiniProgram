/**index.wxss**/
.page {
    background-color: #f6f6f6;
    height: auto;
    z-index: 999;
}

/*搜索框  */
.searchBox{
    position: absolute;
    width: 100%;
    height: 100rpx;
    background: transparent;
    top: 0;
    border: 0;
}

.searchBox .seachText{
    width: 70%;
    height: 60rpx;
    z-index: 1111111;
    background: #fff;
    margin: 20rpx auto;
    border-radius: 50rpx;
    line-height: 60rpx;
}

.searchBox .seachText input{
    width: 60%;
    height: 60rpx;
    font-size: 25rpx;
    padding-left: 30rpx;
    line-height: 60rpx;
}

.searchBox .searchBtn{
    width: 100rpx;
    height: 60rpx;
}

.searchBox .searchBtn image{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 130rpx;
    top: 30rpx;
}

/*轮播图  */
.swiper {
    height: 350rpx;
    width: 100%;
}

.swiper image {
    height: 100%;
    width: 100%;
}

.mp4{
  width: 100%;
