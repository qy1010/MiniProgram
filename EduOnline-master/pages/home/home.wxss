}
.sfcon {
    width: 100%;
    min-height: 350rpx;
    text-align: center;
    padding: 20rpx 0;
    background: #dddddd; 
}

.sfcon image {
    width: 80%;
    height: 100rpx;
}

.sfcon .s-cont{
    min-height: 250rpx;
    font-size: 30rpx;
}

.sfcon .s-cont .st-title{
    display: inline-block;
    background: #1D89E4;
    padding: 10rpx 20rpx;
    border-radius: 10rpx;
    color: white;
    letter-spacing: 5rpx;
    font-family: 'TongXin'
}

.sfcon .s-cont .st-font{
    display: block;
    color:#595959;
    font-size: 28rpx;
    letter-spacing: 5rpx;
    font-family: 'TongXin'
    
}
